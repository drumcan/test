<html>
  <head>
  </head>
  <body>
      <form action="/merchant_create" method="POST" id="braintree-payment-form">
      <select id="merchant_account" name="merchant_account">
        <option value="Approve">Success</option>
        <option value="DeclinedOFAC">Decline OFAC</option>
        <option value="DeclinedMasterCardMatch">Declined Mastercard MATCH</option>
        <option value="DeclinedSsnMatchesDeceased">Declined SSN matches deceased</option>
        <option value="DeclinedFailedKYC">Declined failed KYC</option>
      </select>
      <h2>Verification Information</h2>
      <p>
        <label>First Name</label>
        <input type="text" name="first_name" id="first_name" value="Miguel"></input>
      </p>
      <p>
        <label>Last Name</label>
        <input type="text" name="last_name" id="last_name" value="Cabrera"></input>
      </p>
       <p>
        <label>E-mail</label>
        <input type="text" name="email" id="email" size="30" value="triple_crown_2012@tigers.com"></input>
      </p>
       <p>
        <label>Phone</label>
        <input type="text" name="phone" id="phone" value="555-555-5555"></input>
      </p>
       <p>
        <label>Date of Birth</label>
        <input type="text" name="date_of_birth" id="date_of_birth" size="11" value="1983-04-18"></input>
      </p>
       <p>
        <label>Social Security Number</label>
        <input type="text" name="ssn" id="ssn" value="123456789"></input>
      </p>
       <p>
        <label>Street Address</label>
        <input type="text" name="street_address" id="street_address" size="30" value="2100 Woodward Avenue"></input>
      </p>
       <p>
        <label>City</label>
        <input type="text" name="locality" id="locality" value="Detroit"></input>
      </p>
       <p>
        <label>State</label>
        <input type="text" name="region" id="region" size="2" value="MI"></input>
      </p>
      <p>
        <label>Postal Code</label>
        <input type="text" name="postal_code" id="postal_code" size="5" value="48201"></input>
      </p>
       <p>
        <label>Routing Number</label>
        <input type="text" name="routing_number" id="routing_number" value="002100021"></input>
      </p>
       <p>
        <label>Account Number</label>
        <input type="text" name="account_number" id="account_number" value="1234567"></input>
      </p>
      <input class="submit-button" type="submit" />
    </form>
  </body>
</html>